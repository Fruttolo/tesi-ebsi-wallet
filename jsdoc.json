{
  "source": {
    "include": ["src"],
    "exclude": ["node_modules", "dist", "build", "android"],
    "includePattern": ".+\\.jsx?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "opts": {
    "destination": "./docs/api",
    "recurse": true,
    "readme": "./README.md",
    "template": "default",
    "encoding": "utf8"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  }
}
