{
  "name": "capacitor-app",
  "version": "1.0.0",
  "description": "An Amazing Capacitor App",
  "type": "module",
  "keywords": [
    "capacitor",
    "mobile"
  ],
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "android": "pnpm build && npx cap copy android && npx cap run android",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:run": "vitest run",
    "lint": "eslint src --ext .js,.jsx",
    "lint:fix": "eslint src --ext .js,.jsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,json,css,md}\"",
    "cap:sync": "cap sync",
    "cap:android": "cap open android",
    "android:build": "cd android && ./gradlew assembleRelease",
    "android:build:debug": "cd android && ./gradlew assembleDebug",
    "android:clean": "cd android && ./gradlew clean",
    "security:scan": "snyk test",
    "docs:generate": "jsdoc -c jsdoc.json",
    "analyze": "vite build --mode analyze"
  },
  "dependencies": {
    "@capacitor/android": "^8.0.1",
    "@capacitor/app": "^8.0.0",
    "@capacitor/camera": "latest",
    "@capacitor/core": "^8.0.1",
    "@capacitor/filesystem": "^8.1.0",
    "@capacitor/preferences": "^8.0.0",
    "@capacitor/splash-screen": "latest",
    "@capacitor/status-bar": "^8.0.0",
    "@cef-ebsi/key-did-resolver": "^2.1.1",
    "@cef-ebsi/verifiable-credential": "^7.2.0",
    "@digitalbazaar/vc": "^7.2.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@mattrglobal/bbs-signatures": "^2.0.0",
    "@mattrglobal/jsonld-signatures-bbs": "^1.2.0",
    "@mui/icons-material": "^7.3.7",
    "@mui/material": "^7.3.6",
    "@noble/curves": "^2.0.1",
    "@noble/hashes": "^2.0.1",
    "@scure/bip32": "^2.0.1",
    "@scure/bip39": "^2.0.1",
    "@vitejs/plugin-react": "^5.1.1",
    "base64-js": "^1.5.1",
    "bip39": "^3.1.0",
    "bs58": "^6.0.0",
    "buffer": "^6.0.3",
    "capacitor-secure-storage-plugin": "^0.12.0",
    "date-fns": "^4.1.0",
    "did-jwt": "^8.0.18",
    "did-resolver": "^4.1.0",
    "jose": "^6.1.3",
    "localforage": "^1.10.0",
    "process": "^0.11.10",
    "qrcode": "^1.5.4",
    "qrcode.react": "^4.2.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.10.1",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@capacitor/cli": "^8.0.0",
    "@rollup/plugin-inject": "^5.0.5",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/ui": "^4.0.18",
    "eslint": "^9.39.2",
    "eslint-plugin-no-secrets": "^2.2.1",
    "eslint-plugin-security": "^3.0.1",
    "javascript-obfuscator": "^5.1.0",
    "jsdoc": "^4.0.5",
    "jsdom": "^27.4.0",
    "prettier": "^3.8.1",
    "rollup-plugin-javascript-obfuscator": "^1.0.4",
    "rollup-plugin-visualizer": "^6.0.5",
    "vite": "^5.4.2",
    "vitest": "^4.0.18"
  },
  "author": "",
  "license": "ISC"
}
